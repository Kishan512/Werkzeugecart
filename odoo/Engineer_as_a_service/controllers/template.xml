                            <td>Mobile No</td>
                            <td><t t-esc="order.client_id.mobile_no"/></td>
                        </tr>
                        <tr>
                            <t t-if="order.order_status=='0'">
                                <td>Order Status</td>
                                <td>Order rejected</td>
                            </t>
                            <t t-if="order.order_status=='1'">
                                <td>Order Status</td>
                                <td>Pending</td>
                            </t>
                            <t t-if="order.order_status=='2'">
                                <td>Order Status</td>
                                <td>Accepted</td>
                            </t>
                            <t t-if="order.order_status=='3'">
                                <td>Order Status</td>
                                <td>Delivered</td>
                            </t>
                        </tr>
                        </t>
                    </tbody>
                </table>
                <t t-foreach="order_detail" t-as="order">
                    <t t-if="order.order_status=='1'">
                        <a class="btn btn-success mr-4" t-att-href="'/job_status_change/%s/accept' % order.id">Accept</a>
                        <a class="btn btn-danger" t-att-href="'/job_status_change/%s/decline' % order.id">Decline</a>
                    </t>
                    <t t-if="order.order_status=='2'">
                        Click here to change the status to delivered:
                        <a class="btn btn-danger" t-att-href="'/job_status_change/%s/delivered' % order.id">Delivered</a>
                    </t>
                </t>
            </div>
            <t t-call="Engineer_as_a_service.footer"/>
        </t>
    </template>

    <!-- .............................. profile ..................... -->
    <template id="Engineer_profile">
        <t t-call="web.layout">        
        <t t-call="Engineer_as_a_service.header"/>
            <div class="d-flex justify-content-center mt-5">
                <div class="card text-center" style="width:35em;">
                        <div class="card-header">
                        Profile
                        </div>
                        <div class="card-body">
                            <table style="width:100%">
                                <t t-foreach="profile" t-as="p">
                                    <tr>
                                    <th>Name:</th>
                                    <td><t t-esc="p.name"/></td>
                                    </tr>
                                    <tr>
                                        <th>Mobile:</th>
                                        <td><t t-esc="p.mobile_no"/></td>
                                    </tr>
                                    <tr>
                                        <th>Address:</th>
                                            <td><t t-esc="p.address"/></td>
                                    </tr>
                                    <tr>
                                        <th>Email:</th>
                                        <td><t t-esc="p.email"/></td>
                                    </tr>
                                    <tr>
                                        <th>Specialist:</th>
                                        <td><t t-esc="p.specialist"/></td>
                                    </tr>
                                    <tr>
                                        <th>Experience:</th>
                                        <td><t t-esc="p.experience"/></td>
                                    </tr>
                                </t>
                            </table>
                    </div>
                    <div class="card-footer text-muted">
                    </div>
                </div>
            </div>
            <t t-call="Engineer_as_a_service.footer"/>
        </t>
    </template>


</odoo>