<!-- .............................. orders ................ -->

    <template id="orders">
        <t t-call="web.layout">        
        <t t-call="Engineer_as_a_service.header"/>
            <div class="container">  
                <div class="mt-5 mb-5">
                   <h1>Orders list </h1> 
                </div>
                <div>
                    <table class="table" style="width:50%">
                        <thead>
                            <tr>
                                <th>id</th>
                                <th>Name</th>
                                <th>Order Date</th>
                                <th>Order Time</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                                <t t-set="sr_no" t-value="1"/>
                                <t t-foreach="order_list" t-as="order" t-key="order.id">
                                    <tr>
                                        <td><t t-esc="sr_no"/></td>
                                        <td><t t-esc="order.engineer_id.name"/></td>
                                        <td>
                                            <t t-esc="order.create_date.day"/>/
                                            <t t-esc="order.create_date.month"/>/
                                            <t t-esc="order.create_date.year"/>
                                        </td>
                                        <td>
                                            <t t-esc="order.create_date.hour"/>:
                                            <t t-esc="order.create_date.minute"/> 
                                        </td>
                                        <td><a class="btn btn-success" t-att-href="'/view_order_deatail/%s' % order.id">View</a></td>
                                    </tr>
                                    <t t-set="sr_no" t-value="sr_no+1"/>
                                </t>
                        </tbody>
                    </table>
                </div>      
            </div>
            <t t-call="Engineer_as_a_service.footer"/>
        </t>
    </template>


<!-- ....................................view order detail............................ -->

    <template id="view_order_deatail">
        <t t-call="web.layout">        
        <t t-call="Engineer_as_a_service.header"/>
            <div class="container mt-5">
                <div>
                    <h3 class="container mb-5" style="font-size:36px;">Service Order Detail</h3>
                </div>
                <table class="table table-striped">
                    <thead class="bg-dark text-white">
                        <tr>
                            <th data-field="key" scope="col" class="key">Key</th>
                            <th data-field="value" scope="col">Value</th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="order_list" t-as="order" t-key="task.id">
                        <tr>
                            <td>Order Id</td>
                            <td><t t-esc="order.id"/></td>
                        </tr>
                        <tr>    
                            <td>Name</td>
                            <td><t t-esc="order.engineer_id.name"/></td>
                        </tr>
                        <tr>
                            <td>Mobile No</td>
                            <td><t t-esc="order.engineer_id.mobile_no"/></td>
                        </tr>
                        <tr>
                            <td>Order Date</td>
                            <td>
                                <t t-esc="order.create_date.day"/>/
                                <t t-esc="order.create_date.month"/>/
                                <t t-esc="order.create_date.year"/>
                            </td>
                        </tr>
                        <tr>
                            <td>Order Time</td>
                            <td>
                                <t t-esc="order.create_date.hour"/>:
                                <t t-esc="order.create_date.minute"/> 
                            </td>
                        </tr>
                        <tr>
                            <td>Specialist</td>
                            <td><t t-esc="order.engineer_id.specialist"/></td>
                        </tr>
                        <tr>
                            <td>experience</td>
                            <td><t t-esc="order.engineer_id.experience"/></td>
                        </tr>
                        </t>
                    </tbody>
                </table>
                <table class="table table-striped mt-2">
                    <thead class="bg-dark text-white">
                        <tr>
                            <th>Order Staus</th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="order_list" t-as="order">
                            <tr>
                                <td>
                                    <t t-if="order.order_status=='1'">
                                        Pending        
                                    </t>
                                    <t t-if="order.order_status=='2'">
                                        Accepted
                                    </t>
                                    <t t-if="order.order_status=='3'">
                                         Delivered
                                    </t>   
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>
            </div>
        <t t-call="Engineer_as_a_service.footer"/>
        </t>
    </template>

